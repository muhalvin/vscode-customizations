{
    "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
    "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-dark",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.colorCustomizations": {
        "inlineparameters.annotationBackground": "#282A3600",
        "inlineparameters.annotationForeground": "#7b82af",
        "scrollbar.shadow": "#24292e00"
    },
    "workbench.editorLargeFileConfirmation": 10024,
    "workbench.layoutControl.enabled": false,
    "workbench.tree.indent": 20,
    "workbench.startupEditor": "none",
    "workbench.editor.empty.hint": "hidden",
    "workbench.activityBar.location": "top",
    /** ------------ Breadcrumbs Configurations ------------ **/
    "breadcrumbs.enabled": false,
    "breadcrumbs.symbolPath": "off",
    "breadcrumbs.filePath": "on",
    /** ------------ Editor Settings ------------ **/
    "editor.fontFamily": "Jetbrains Mono",
    "editor.fontSize": 16,
    "editor.fontWeight": 450,
    "editor.lineHeight": 25,
    "editor.lineNumbers": "on",
    "editor.folding": true,
    "editor.glyphMargin": true,
    "editor.guides.indentation": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.minimap.enabled": true,
    "editor.wordWrap": "on",
    "editor.stickyScroll.enabled": true,
    "editor.tabSize": 4,
    "multiDiffEditor.experimental.enabled": false,
    "editor.formatOnSave": true,
    "editor.cursorBlinking": "smooth",
    /** ------------ Terminal Configurations ------------ **/
    "terminal.integrated.fontFamily": "'Jetbrains Mono', Monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.enableMultiLinePasteWarning": "always",
    "terminal.integrated.cursorStyle": "line",
    /** ------------ Php Intelephense ------------ **/
    "files.associations": {
        "*.module": "php",
        "*.f7": "html",
        "*.php": "php",
        "*.blade.php": "blade"
    },
    "json.schemas": [],
    /** ------------ Devdb ------------ **/
    "Devdb.colorTheme": "dark",
    "Devdb.dontShowNewVersionMessage": true,
    /** ------------ Laravel Blade Formatter ------------ **/
    "bladeFormatter.format.wrapLineLength": 150,
    "bladeFormatter.misc.dontShowNewVersionMessage": true,
    "bladeFormatter.format.indentSize": 4,
    /** ------------ Laravel Blade Snippet ------------ **/
    "blade.format.enable": false,
    /** ------------ Laravel GoTo View ------------ **/
    "laravel_goto_view.maxLinesCount": 500,
    /** ------------ CSS Peek ------------ **/
    "cssPeek.peekFromLanguages": [
        "html",
        "django-html",
        "laravel-blade",
        "razor",
        "vue",
        "blade",
        "pug",
        "jade",
        "handlebars",
        "php",
        "twig",
        "md",
        "nunjucks",
        "javascript",
        "javascriptreact",
        "erb",
        "typescript",
        "typescriptreact",
        "HTML (Eex)",
        "html-eex",
        "hbs",
        "svelte"
    ],
    /** ------------ Eslint ------------ **/
    "eslint.format.enable": true,
    /** ------------ Additional Configurations ------------ **/
    "vscode_custom_css.imports": [
        "file:///C:/Users/Muhammad%20Alvin/.vscode/custom/style.css"
    ]
}